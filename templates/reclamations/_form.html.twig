<div class="container">
{{form_start(form,{'attr':{'novalidate':'novalidate'}})}}
 <div class="row px-xl-5">
            <div class="col-lg-7 mb-5">
                <div class="contact-form bg-light p-30">
<div class="form-group">
    {{ form_label(form.etat,"etat") }}
    {{ form_widget(form.etat,{ 'attr': {'class': 'form-control'} }) }} 
    {% if form_errors(form.etat) is not empty %}
    <div class="alert alert-danger" >
    {{ form_errors(form.etat) }}

{% endif %}
</div>
<div class="form-group">
    {{ form_label(form.description,"descritption") }}
    {{ form_widget(form.description,{ 'attr': {'class': 'form-control' , 'rows': 8} }) }} 
    {% if form_errors(form.description) is not empty %}
    <div class="alert alert-danger" >
    {{ form_errors(form.description) }}
</div>   
{% endif %}
</div>
<br>
    <button class="btn btn-primary py-2 px-4">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
</div>
 </div>
