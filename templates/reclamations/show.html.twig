{% extends 'base.html.twig' %}

{% block title %}
  Reclamations
{% endblock %}

{% block body %}
  <h1>
    Reclamations
  </h1>

  <table class="table">
    <tbody>
      <tr>
        <th>
          Etat
        </th>
        <td>
          {{ reclamation.etat }}
        </td>
      </tr>
      <tr>
        <th>
          Description
        </th>
        <td>
          {{ reclamation.description }}
        </td>
      </tr>
    </tbody>
  </table>

  <a href="{{ path('app_reclamations_index') }}">back to list</a>

  <a href="{{
    path(
      'app_reclamations_edit',
      {
        id: reclamation.id
      }
    )
    }}">
    edit
  </a>

  {{ include('reclamations/_delete_form.html.twig') }}
{% endblock %}
